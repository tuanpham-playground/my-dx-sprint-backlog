{
  "manifest_version": 1,
  "version": "5.0.2",
  "type": "APP",
  "desktop": {
    "js": [
      "https://js.cybozu.com/jquery/1.11.1/jquery.min.js",
      "js/desktop.js"
    ],
    "css": [
      "css/desktop.css"
    ]
  },
  "icon": "image/icon.png",
